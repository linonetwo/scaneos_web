{
  "name": "scaneos_web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*",
    "start": "yarn clear:babel-cache && react-app-rewired start",
    "build": "yarn clear:babel-cache && react-app-rewired build",
    "postbuild": "gzip-all ./build/**/*.css && gzip-all ./build/**/*.js && gzip-all ./build/**/*.png && react-snap",
    "test": "yarn clear:babel-cache && react-app-rewired test --env=jsdom",
    "docker-build": "docker build . -t scaneos_web",
    "docker-run": "docker run --rm --name scaneos_web -p 80:80 -p 443:443 scaneos_web",
    "eject": "react-scripts eject"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--disable-setuid-sandbox",
      "--no-sandbox"
    ],
    "concurrency": 1,
    "include": [
      "/",
      "/blocks",
      "/transactions",
      "/accounts",
      "/messages",
      "/producers"
    ],
    "crawl": false,
    "inlineCss": false,
    "puppeteerExecutablePath": "/usr/bin/chromium-browser"
  },
  "dependencies": {
    "@rematch/core": "^1.0.0-alpha.5",
    "@rematch/immer": "^1.0.2",
    "antd": "^3.6.1",
    "babel-eslint": "^8.2.3",
    "camelize": "^1.0.0",
    "date-fns": "^1.29.0",
    "echarts": "^4.1.0",
    "ethereumjs-abi": "^0.6.5",
    "flow-bin": "^0.73.0",
    "flow-runtime": "^0.17.0",
    "gzip-all": "^1.0.0",
    "i18next": "^11.3.2",
    "i18next-browser-languagedetector": "^2.2.0",
    "lodash": "^4.17.10",
    "no-scroll": "^2.1.1",
    "numeral": "^2.0.6",
    "preval.macro": "^1.0.2",
    "react": "^16.4.0",
    "react-autolink-text2": "^3.2.0",
    "react-countdown-now": "^1.3.0",
    "react-datasheet": "^1.3.7",
    "react-dom": "^16.4.0",
    "react-echarts-v3": "^1.0.19",
    "react-i18next": "^7.7.0",
    "react-map-gl": "^3.2.8",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.0-next.66cc7a90",
    "styled-components": "3.3.2",
    "styled-components-breakpoint": "^2.0.2",
    "styled-components-mixins": "^0.0.5",
    "styled-flex-component": "^2.2.2",
    "styled-is": "^1.1.3",
    "styled-normalize": "^4.0.0",
    "web3": "^1.0.0-beta.34"
  },
  "devDependencies": {
    "@babel/plugin-proposal-do-expressions": "^7.0.0-beta.46",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.47",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.46",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0-beta.46",
    "babel-eslint": "^8.2.3",
    "babel-plugin-closure-elimination": "^1.3.0",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "flow-typed": "^2.4.0",
    "react-app-rewire-less": "^2.1.1",
    "react-app-rewired": "^1.5.2",
    "react-cosmos": "^4.4.0",
    "react-cosmos-fetch-proxy": "^4.4.0",
    "react-cosmos-redux-proxy": "^4.4.0",
    "react-cosmos-router-proxy": "^4.4.0",
    "react-snap": "^1.13.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
